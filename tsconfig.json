{
  "extends": "astro/tsconfigs/strictest",
  "include": [
    ".astro/types.d.ts",
    "src/**/*.ts",
    "src/**/*.js",
    "src/**/*.astro",
    "src/**/*.tsx",
    "src/**/*.jsx"
  ],
  "exclude": [
    "dist",
    "node_modules",
    ".astro",
    "**/*.d.ts"
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"]
    }
  }
}
